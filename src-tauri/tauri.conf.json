{
  "$schema": "https://raw.githubusercontent.com/nicholastmosher/tauri/HEAD/crates/tauri-utils/schema.json",
  "productName": "Audiotester",
  "version": "0.1.5",
  "identifier": "com.audiotester.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:8920"
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "title": "Audiotester",
        "url": "http://localhost:8920",
        "width": 800,
        "height": 600,
        "visible": false,
        "resizable": true,
        "minWidth": 400,
        "minHeight": 300
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": ["nsis"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico",
      "icons/icon.png"
    ],
    "windows": {
      "nsis": {
        "installMode": "currentUser"
      }
    }
  }
}
